#!/bin/sh
LD_LIBRARY_PATH="@@BITROCK_COMMON_ROOTDIR@@/lib:$LD_LIBRARY_PATH"	   
DYLD_LIBRARY_PATH="@@BITROCK_COMMON_ROOTDIR@@/lib:$DYLD_LIBRARY_PATH"	   
OPENSSL_CONF="@@BITROCK_COMMON_ROOTDIR@@/openssl/openssl.cnf"
export LD_LIBRARY_PATH
export DYLD_LIBRARY_PATH
export OPENSSL_CONF
exec @@BITROCK_COMMON_ROOTDIR@@/bin/openssl.bin "$@"
